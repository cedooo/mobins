<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.dhcc.cgn.mobins.report.dao.InspectionReport">
	<resultMap type="cn.com.dhcc.cgn.mobins.report.dao.InspectionReport" id="InspectionReportMap">
		<result property="inspectionReportID" column="N_INSPECTION_REPORT_ID" />
		<result property="maintAccount" column="VC_MAINT_ACOUNT" />
		<result property="maintUser" column="VC_MAINT_USER" />
		<result property="generateTime" column="DT_GENERATE_TIME" />
		<result property="inspectionTime" column="DT_INSPECTION_TIME" />
		<result property="isException" column="VC_INSPECTION_IS_EXCEPTION" />
	</resultMap>

	<select id="selectAll" resultMap="InspectionReportMap">
		SELECT N_INSPECTION_REPORT_ID
	      ,VC_MAINT_ACOUNT
	      ,VC_MAINT_USER
	      ,DT_GENERATE_TIME
	      ,DT_INSPECTION_TIME
	      ,VC_INSPECTION_IS_EXCEPTION
	  	FROM T_INSPECTION_REPORT
	</select>

</mapper>