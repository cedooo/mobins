<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- services -->
	<!-- 报告单 -->
	<bean id="reportService"
		class="cn.com.dhcc.cgn.mobins.report.service.impl.ReportServiceImpl" >
	</bean>

	<!-- 巡检目标  -->
	<bean id="targetService"
		class="cn.com.dhcc.cgn.mobins.setting.service.impl.TargetServiceImpl" >
	</bean>
	
	<!-- 巡检主机  -->
	<bean id="hostService"
		class="cn.com.dhcc.cgn.mobins.setting.service.impl.MobDestHostServiceImpl" >
	</bean>

	<!-- 巡检策略  -->
	<bean id="strageService"
		class="cn.com.dhcc.cgn.mobins.setting.service.impl.StrageServiceImpl" >
	</bean>
	<!-- 巡检巡检项目  -->
	<bean id="inspectionItemService"
		class="cn.com.dhcc.cgn.mobins.setting.service.impl.InspectionItemServiceImpl" >
	</bean>
	
	<!-- 巡检巡检项目 巡检点  -->
	<bean id="inspectionPointService"
		class="cn.com.dhcc.cgn.mobins.setting.service.impl.InspectionPointServiceImpl" >
	</bean>
	
	
	<!-- 巡检巡检项目 应用  -->
	<bean id="inspectionItemApplyService"
		class="cn.com.dhcc.cgn.mobins.setting.service.impl.InspectionItemApplyServiceImpl" >
	</bean>
	
	
	<!-- 巡检策略 应用  -->
	<bean id="strageApplyService"
		class="cn.com.dhcc.cgn.mobins.setting.service.impl.StrageApplyServiceImpl" >
	</bean>
	<!--  -->
	<!-- PO -->
	
	<!-- 巡检点目标 -->
	<bean id="target"
		class="cn.com.dhcc.cgn.mobins.po.MobInsTarget" >
	</bean>
	<!-- 巡检主机 -->
	<bean id="mobDestHost"
		class="cn.com.dhcc.cgn.mobins.po.MobDestHost" >
	</bean>
	<!-- 巡检策略 -->
	<bean id="strage"
		class="cn.com.dhcc.cgn.mobins.po.InspectionStrage" >
	</bean>
	
</beans>